#!/usr/bin/env bash
#
# Sort records w/ Sort for Fzf
# ============================
# Tim Friske <me@tifr.de>
#
# Dependencies::
#   _Bash_

shopt -os nounset pipefail errexit errtrace

function print_help {
}

function fzf_sort {
  if [[ "${1:-}" == '--help' ]]; then
    print_help
    exit
  fi
}

fzf_sort "$@"
